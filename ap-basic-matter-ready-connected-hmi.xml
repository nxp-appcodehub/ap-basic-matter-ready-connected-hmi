<data:item xmlns:data="data" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="data ../../xsd/alp.xsd" id="ap-basic-matter-ready-connected-hmi" type="sw_pack" format="projectfile" name="Basic Matter Ready Connected HMI">
    <display_title>
        Basic Matter Ready Connected HMI platform with cost-optimized MCU, Wi-Fi 4, Thread, and Bluetooth Low Energy 5.0
    </display_title>
    <overview>
        <![CDATA[NXP's cost-optimized MCU (FreeRTOS)
  platform enables Matter-ready edge node development with Wi-Fi 4, Thread, and Bluetooth Low Energy 5.0 wireless connectivity protocols using i.MX RT1060 MCU, K32W0x MCU, and 88W8801 Wi-Fi SoC.]]>
    </overview>
    <toolchains>
		<toolchain name="gcc"/>
    </toolchains>
    <families>
        <family name="imxrt"></family>
        <family name="k32w"></family>
        <family name="mw"></family>
    </families>
    <nxp_compatible_hw>
        <hw id="EVK-MIMXRT1060"></hw>
    </nxp_compatible_hw>
    <nxp_ebh_hw>
        <ebh id="murata_murata_88w8801_module"></ebh>
    </nxp_ebh_hw>
    <peripherals>
        <peripheral name="display"></peripheral>
        <peripheral name="dma"></peripheral>
        <peripheral name="flash"></peripheral>
        <peripheral name="gpio"></peripheral>
        <peripheral name="i2c"></peripheral>
        <peripheral name="spi"></peripheral>
        <peripheral name="uart"></peripheral>
        <peripheral name="usb"></peripheral>
    </peripherals>
    <categories>
        <category name="wireless_connectivity"></category>
        <category name="hmi"></category>
        <category name="cc_devices"></category>
        <category name="rtos"></category>
    </categories>
</data:item>
